{
    "name": "scalexy/filament-spatie-media-library-croppie",
    "description": "A filament form field for image upload using spatie media library and croppie.js",
    "keywords": [
        "laravel",
        "filament",
        "croppie",
        "file-upload",
        "spatie-media-library"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ScaleXY\\FilamentSpatieMediaLibraryCroppie\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nuhel",
            "email": "nuheljft@gmail.com"
        },
        {
            "name": "Josef Behr",
            "email": "code@josef-behr.de"
        },
        {
            "name": "Athul Raj",
            "email": "athul@scalexy.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/spatie-laravel-media-library-plugin": "^2.0",
        "spatie/laravel-medialibrary": "^10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScaleXY\\FilamentSpatieMediaLibraryCroppie\\FilamentSpatieMediaLibraryCroppieServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true

}
